{
  "name": "lo-utils",
  "version": "1.0.0-dev",
  "description": "An utils",
  "bin": {
    "lo-utils": "./bin/cli.js"
  },
  "sideEffects": false,
  "main": "index.js",
  "module": "dist/lo-utils.esm.js",
  "browser": "dist/lo-utils.browser.esm.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest --watch",
    "build": "tsx ./build",
    "build:module": "rollup -c",
    "build:dts": "tsc && rollup -c rollup.config.dts.mjs",
    "build:doc": "tsc && api-extractor run --local --verbose && api-documenter markdown -i ./dist -o ./doc",
    "clean": "rimraf ./dist"
  },
  "author": "luojunbang",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luojunbang/lo-utils.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/runtime": "^7.28.4",
    "@jest/globals": "^30.1.2",
    "@microsoft/api-documenter": "^7.26.33",
    "@microsoft/api-extractor": "^7.52.12",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/fs-extra": "^11.0.4",
    "core-js": "^3.45.1",
    "execa": "^8.0.1",
    "rollup": "^4.50.1",
    "rollup-plugin-dts": "^6.2.3",
    "ts-jest": "^29.4.1",
    "tslib": "^2.8.1",
    "unbuild": "^3.6.1",
    "vue": "^3.5.21"
  },
  "peerDependencies": {
    "dotenv": ">=16",
    "fast-glob": ">=3",
    "fs-extra": ">=11",
    "vue": ">=3"
  },
  "dependencies": {
    "@types/jest": "^30.0.0",
    "inquirer": "^12.9.4",
    "jest": "^30.1.3",
    "ts-node": "^10.9.2",
    "xlsx": "^0.18.5"
  }
