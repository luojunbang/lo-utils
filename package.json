{
  "name": "lo-utils",
  "version": "1.2.15",
  "description": "A Utils by Lo",
  "main": "dist/lo-utils.min.js",
  "module": "dist/lo-utils.esm.js",
  "types": "dist/lo-utils.d.ts",
  "scripts": {
    "doc": "node ./build/generator_doc.js",
    "build": "rollup -c rollup.config.js && npm run build:dts",
    "build:dts": "api-extractor run --local --verbose",
    "dev": "webpack --watch --config ./build/webpack.config.js",
    "vite": "vite --config ./example/vite.config.ts",
    "test": "mocha --watch",
    "release": "npm run build && npm run publish:bug",
    "publish:bug": "npm version patch && npm publish"
  },
  "author": "luojunbang",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@microsoft/api-extractor": "^7.28.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "axios": "^0.21.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "esbuild": "^0.14.49",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "file-system": "^2.2.2",
    "mocha": "^9.0.0",
    "mysql": "^2.18.1",
    "path-browserify": "^1.0.1",
    "pify": "^5.0.0",
    "prettier": "^2.3.2",
    "rollup": "^2.76.0",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vue": "^3.2.37",
    "vue-eslint-parser": "^9.0.3",
    "vue-router": "^4.1.1",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2"
  },
  "files": [
    "dist/lo-utils.d.ts",
    "dist/lo-utils.*.js",
    "style/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luojunbang/lo-utils.git"
  },
  "eslintIgnore": [
    "/dist/**"
  ]
}
