{
  "includes": ["src/global.d.ts","src/**.ts"],
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "dist",
    "module": "es6",
    "target": "es6",
    "strict": true,
    "moduleResolution": "node",
    "noImplicitThis": false,
  },
  "types":[
    "vite/client"
  ],
  "paths": {
    "@/*": [
      "src/*"
    ]
  }
}
